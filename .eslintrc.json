{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "eol-last": 0,
        "no-multiple-empty-lines": ["error", 
        { "max": 1, "maxEOF": 0 }],
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "max-len": "off"
    }
}
